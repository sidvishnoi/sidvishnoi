{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"downlevelIteration": true,
		"useUnknownInCatchVariables": false,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true
	}
}
